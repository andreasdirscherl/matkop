<script type="text/javascript">

function animateSlogan() {
   $(document).ready(function() {
	animateImages('#sloganKonkret');
	window.setTimeout("animateImages('#sloganWirksam')", 6000);
	window.setTimeout("animateImages('#sloganVielseitig')", 12000);					    window.setTimeout("animateSlogan()", 19000);
})
}

function animateImages(id) {
	 $(id).animate({
		// Grow.
		opacity: 1,
		height: "30px",
		width: "120px"
	  }, 4000, function() {
		// Shrink 
	 $(id).animate({
		opacity: .3,
		height: "23px",
		width: "90px"
	  }, 6000, function() {
	  });
	  });
}


